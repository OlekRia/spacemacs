{
    "gitbook": ">=3.0.0",
    "title": "Clojure development with Spacemacs & Cider",
    "description": "Speedy Clojure development in Spacemacs, covering all the features you need for daily hugging of your code",
    "structure": {
        "readme": "introduction.md"
    },
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "plugins": [
        "callouts",
        "codeblock-disable-glossary",
        "ga",
        "gist",
        "heading-anchors",
        "klipse",
        "sectionx",
        "simpletabs",
        "toggle-chapters",
        "toolbar",
        "youtube",
        "wide-page"
    ],
    "pluginsConfig": {
        "callouts": {
            "showTypeInHeader": false,
            "star": {
                "alert": "warning",
                "picto": "fa-star"
            }
        },
        "sectionx": {
            "tag": "b"
        },
        "ga": {
            "token": "UA-28030709-9"
        },
       "toolbar": {
           "buttons":
           [
               {
                   "label": "Clojurians Slack",
                   "icon": "fa fa-comments-o",
                   "url": "http://clojurians.slack.com/messages/practicalli"
               },
               {
                   "label": "GitHub",
                   "icon": "fa fa-github",
                   "url": "https://github.com/practicalli/spacemacs-content/projects/1"
               }
           ]
       }
    },
    "links": {
        "sidebar": {
            "Spacemacs (develop)": "http://develop.spacemacs.org/",
            "Practical.li": "https://practicalli.github.io"
        }
    },
    "variables": {
        "host": "practical.li",
        "url": "https://practical.li/spacemacs",
        "P9ISpacemacsD": "https://github.com/practicalli/spacemacs.d",
        "P9IClojure": "https://practical.li/clojure/",
        "P9IClojureDepsEdn": "https://github.com/practicalli/clojure-deps-edn",
        "P9IClojureDepsEdnInstall": "https://practical.li/clojure/clojure-cli/install/community-tools.html",
        "P9IClojureThroughCode": "https://github.com/practicalli/clojure-through-code",
        "P9IIssuesUrl": "https://github.com/practicalli/spacemacs-content/issues",
        "P9IPullRequestsUrl": "https://github.com/practicalli/spacemacs-content/pulls"
    }
}
